{
  "name": "system-behavior",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "clean": "read -p \"Are you sure you want to clean the project? [y/n] \" REPLY; if [[ $REPLY =~ ^[Yy]$ ]]; then rm -R .git .github demo-app test .codeclimate.yml .travis.yml jest.json wdio.BUILD.conf.js src/features/**; fi",
    "test": "run-s test:*",
    "test:lint": "eslint --fix 'src/**/*.ts'",
    "test:unit": "jest --config=jest.config.cjs --detectOpenHandles",
    "test:features": "node --loader ts-node/esm ./node_modules/.bin/wdio run wdio.BUILD.conf.ts",
    "wdio": "node --loader ts-node/esm ./node_modules/.bin/wdio run wdio.BUILD.conf.ts"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@wdio/cli": "^8.0.13",
    "@wdio/config": "^8.0.13",
    "@wdio/cucumber-framework": "^8.0.13",
    "@wdio/globals": "^8.0.13",
    "@wdio/local-runner": "^8.0.13",
    "@wdio/selenium-standalone-service": "^8.0.13",
    "@wdio/spec-reporter": "^8.0.13",
    "@wdio/static-server-service": "^8.0.11",
    "@wdio/types": "^8.0.11",
    "chromedriver": "^108.0.0",
    "eslint": "^8.30.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-wdio": "^8.0.7",
    "http-server": "^14.1.1",
    "jest": "^29.3.1",
    "jest-circus": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "wdio-chromedriver-service": "^8.0.1",
    "webdriverio": "^8.0.13"
  }
}
